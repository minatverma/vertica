--- AFFILIATION table definition
--- 
CREATE TABLE public.AFFILIATION
(
    AFFILIATION_ID INT NOT NULL,
    "NAME" varchar(100) NOT NULL,
    URL1 varchar(100),
    URL2 varchar(100),
    URL3 varchar(100),
    URL4 varchar(100),
    URL5 varchar(100),
    INVITE_URL varchar(100) NOT NULL,
    EMAIL varchar(50) NOT NULL,
    "ENABLED" BOOLEAN,
    IMAGE_URI varchar(255),
    HOME_PAGE_IMAGE_URI varchar(255),
    OBJ_VERSION INT DEFAULT (0),
    REG_STARTPAGE_IMAGE_URI varchar(255),
    DELETED BOOLEAN,
    PARTNER_ID INT,
    BBY_OSS_REGISTER_ID varchar(255)
);


