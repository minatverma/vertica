--- PARTNER table definition
---
CREATE TABLE public.PARTNER
(
    PARTNER_ID int NOT NULL,
    OBJ_VERSION int,
    PARTNER_NAME varchar(255),
    PARTNER_KEY varchar(255),
    PARTNER_DOMAIN_NAME varchar(255),
    YOURPAY_STORE_NAME varchar(255),
    "ENABLED" boolean,
    DELETED boolean,
    USE_DEFAULT_EXPERTS boolean,
    USE_DEFAULT_SERVICES boolean,
    HIDE_SOLUTIONS boolean,
    PUBLIC_SOLUTIONS boolean,
    SCORE_METHOD varchar(255) DEFAULT 'SupportSpace',
    MAX_CONCURRENT_SESSIONS int,
    SHOW_MARKETING_HTML boolean,
    PARTNER_EMAIL varchar(255),
    SERVICE_OPTION varchar(255),
    DEFAULT_AIU varchar(255),
    REPORTING_USER varchar(255),
    REMOTE_TOOL varchar(255),
    SHOW_EXPERT_PRICING boolean,
    MAX_CONCURRENT_SESSIONS_VWF boolean DEFAULT true,
    REMOTE_TOOL_VWF varchar(255) DEFAULT 'Webex',
    SERVICE_BUY_NOW_OPTION varchar(255),
    ALLOW_CUSTOM_JOBOFFER_PRICE boolean,
    SHOW_VWF boolean,
    CHECK_POPUP_BLOCKER boolean DEFAULT false,
    PHONE_2_SERVICE_PHONE varchar(255),
    PHONE_2_SERVICE_TEXT varchar(255),
    LOCALE varchar(255),
    CYBERSOURCE_ACCOUNT_NAME varchar(255),
    CYBERSOURCE_PUBLIC_KEY varchar(255),
    CYBERSOURCE_SERIAL_NUM varchar(255),
    PAYMENT_PROVIDER varchar(255) DEFAULT 'FIRSTDATA',
    DEFAULT_CURRENCY varchar(255) DEFAULT 'USD',
    DEFAULT_COUNTRY int DEFAULT 83,
    CYBERSOURCE_PATH varchar(255),
    SHOW_LANGUAGE boolean DEFAULT false,
    REPORTING_PREFIX varchar(10),
    USE_DEFAULT_COUNTRY_ONLY boolean DEFAULT false,
    TAX_METHOD varchar(255) DEFAULT 'CUSTOMER',
    USE_GENERIC_HOMEPAGE boolean DEFAULT false,
    COOKIE_CUTTER boolean DEFAULT false,
    PHOTO_URI varchar(255),
    USER_NAME varchar(50) NOT NULL,
    "PASSWORD" varchar(40),
    EMAIL varchar(60),
    PARTNER_SUPPORTER_TOOLS varchar(4000),
    PARTNER_SUPPORTER_TOOLS_VWF varchar(4000),
    SHOW_VWF_IN_P2S boolean DEFAULT false,
    USE_SHARED_PARTNER_FOLDER boolean DEFAULT false,
    SHOW_BREADCRUMB boolean DEFAULT false,
    USE_PRE_SESSION_WORKBENCH boolean DEFAULT true,
    USE_P2S_PRE_SESSION_WORKBENCH boolean DEFAULT true,
    LMI_ACCOUNT_ID int,
    SELL_SUBSCRIPTION boolean DEFAULT false,
    COOKIE_CUTTER_CUSTOM_CMS_PAGES boolean DEFAULT false,
    USE_SHORT_REGISTRATION boolean,
    EXTERNAL_HEADER_FOOTER_URL varchar(255),
    JABBER_HTTPBIND_PATH varchar(50),
    PASSWORD_POLICY varchar(40) NOT NULL DEFAULT 'MEDIUM',
    CYBERSOURCE_REPORT_PASSWORD varchar(40),
    CYBERSOURCE_REPORT_USERNAME varchar(40),
    TRANSACTION_RECOVERY_ENABLED boolean DEFAULT false,
    ENFORCE_TERMS_AGREEMENT boolean DEFAULT false,
    REMEMBER_ME_CHECKED boolean DEFAULT true,
    CHAT_AUTO_CHOOSE_MODE varchar(15),
    CUSTOMER_PHONE_MANDATORY boolean DEFAULT false,
    EXPERT_LIST_PAGE_SIZE int,
    SERVICE_LIST_PAGE_SIZE numeric(37,15),
    SHOW_ONLY_ONLINE_EXPERTS boolean DEFAULT false,
    CUSTOMER_HOLD_LMI_WHEN_PAYING boolean DEFAULT false,
    LOGIN_FORM_URL varchar(255),
    DISP_CUST_RECON_ON_SUP_DISCON boolean NOT NULL DEFAULT false,
    BASE_64_AUTH_SECRET_KEY varchar(256),
    AUTH_REQUIRED_TO_BROWSE boolean DEFAULT false
);



