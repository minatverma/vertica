--- TRANSACTION table definition
---
CREATE TABLE public."TRANSACTION"
(
    TRANSACTION_ID int NOT NULL,
    PAYER_USER_ID int,
    PAYEE_USER_ID int,
    SUPPORT_SESSION_ID int,
    AMOUNT int NOT NULL,
    PAYMENT_DATE date,
    APPROVAL_CODE varchar(128),
    PAYMENT_STATUS varchar(255),
    FAIL_REASON varchar(1024),
    OID varchar(128),
    TRANSACTION_TIME varchar(128),
    PAYMENT_PROVIDER varchar(255),
    OBJ_VERSION int DEFAULT 0,
    GRANTED_SYS_PRE_PAID_COUPON_ID int,
    CURRENCY varchar(255) NOT NULL DEFAULT 'USD',
    COUNTRY_ID int,
    ZIP_CODE varchar(20),
    STATE_ID int,
    BASE_AMOUNT int,
    TAX_AMOUNT float,
    TAX_PERCENTAGE float,
    PAYMENT_OPTION_ID int,
    PARTNER_ID int,
    RECONCILIATION_ID varchar(128),
    AUTHORIZATION_CODE varchar(128),
    PREPAID_CARD_TYPE varchar(255),
    PAYMENT_CARD_ID int,
    EXTERNAL_TRANSACTION_ORDER_ID varchar(512),
    PAYMENT_CHANNEL varchar(40)
);


